{
  "manifest_version": 3,
  "icons": {
    "16": "logo16.png",
    "64": "logo64.png",
    "128": "logo128.png",
    "192": "logo192.png"
  },
  "permissions": ["management", "notifications", "storage", "tabs"],
  "background": {
    "service_worker": "./background.bundle.js"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["./contentScript.bundle.js", "./sdk.bundle.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "action": {
    "default_icon": {
      "16": "logo16.png",
      "64": "logo64.png",
      "128": "logo128.png",
      "192": "logo192.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["sdk.bundle.js"],
      "matches": ["<all_urls>"]
    }
  ],
