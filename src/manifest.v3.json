{
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "topSites"],
  "content_security_policy": "default-src 'self'; frame-ancestors 'none';",
  "icons": {
    "16": "assets/img/logo16.png",
    "64": "assets/img/logo64.png",
    "128": "assets/img/logo128.png",
    "192": "assets/img/logo192.png"
  },
  "action": {
    "default_popup": "./apps/popup/index.html",
    "default_icon": {
      "16": "assets/img/logo16.png",
      "64": "assets/img/logo64.png",
      "128": "assets/img/logo128.png",
      "192": "assets/img/logo192.png"
    }
  },
  "background": {
    "service_worker": "./background/index.ts",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["./content/index.ts"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["./sdk/index.ts"],
      "matches": ["<all_urls>"]
    }
  ],
